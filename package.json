{
	"name": "mp-express-proxy",
	"version": "1.0.0",
	"description": "a proxy for communicating with mixpanel's js SDK",
	"main": "server.js",
	"scripts": {
		"start": "RUNTIME=prod node server.js",
		"dev": "RUNTIME=dev nodemon --inspect server.js",
		"frontend": "npx serve ./demo/",
		"test": "jest",
		"stress": "npx artillery run --dotenv ./performance/.env --output ./performance/report.json ./performance/proxy-load-test.yml",
		"deploy:cloudrun": "./scripts/deploy-cloudrun.sh",
		"deploy:cloudfunc": "./scripts/deploy-cloudfunc.sh",
		"deploy:aws": "./scripts/deploy-aws.sh",
		"deploy:azure": "./scripts/deploy-azure.sh"
	},
	"keywords": [
		"mixpanel",
		"proxy",
		"express",
		"cloudrun",
		"nodejs",
		"javascript",
		"trakcer",
		"analytics",
		"events"
	],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@google-cloud/functions-framework": "^3.4.0",
		"azure-function-express": "^2.0.0",
		"body-parser": "^1.20.2",
		"dotenv": "^16.4.5",
		"express": "^4.18.2",
		"fetch-retry": "^5.0.6",
		"http-proxy-middleware": "^2.0.6",
		"serverless-http": "^3.2.0"
	},
	"jest": {
		"globalSetup": "./tests/setup.js",
		"globalTeardown": "./tests/teardown.js"
	},
	"devDependencies": {
		"ak-tools": "^1.0.58",
		"artillery": "^2.0.12",
		"jest": "^29.7.0",
		"nodemon": "^3.1.0",
		"puppeteer": "^22.9.0",
		"tree-kill": "^1.2.2"
	}
}
